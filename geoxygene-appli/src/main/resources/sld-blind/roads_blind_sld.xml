        </Rule>
      </FeatureTypeStyle>	
      <FeatureTypeStyle>  
        <Rule>
          <Filter>
            <And>
              <PropertyIsEqualTo>
                <PropertyName>highway</PropertyName>
                <Literal>track</Literal>
              </PropertyIsEqualTo>
              <PropertyIsEqualTo>
                <PropertyName>tracktype</PropertyName>
                <Literal>grade1</Literal>
              </PropertyIsEqualTo>
            </And>
          </Filter>
          <LineSymbolizer>
            <Stroke>
              <CssParameter name="stroke">#cccccc</CssParameter>
              <CssParameter name="stroke-width">4</CssParameter>
              <CssParameter name="stroke-linecap">butt</CssParameter>
            </Stroke>
          </LineSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <And>
              <PropertyIsEqualTo>
                <PropertyName>highway</PropertyName>
                <Literal>track</Literal>
              </PropertyIsEqualTo>
              <PropertyIsEqualTo>
                <PropertyName>tracktype</PropertyName>
                <Literal>grade2</Literal>
