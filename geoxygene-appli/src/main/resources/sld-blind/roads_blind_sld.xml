              <CssParameter name="stroke-linecap">butt</CssParameter>
              <CssParameter name="stroke-dasharray">3 4</CssParameter>  
            </Stroke>
          </LineSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <And>
              <PropertyIsEqualTo>
