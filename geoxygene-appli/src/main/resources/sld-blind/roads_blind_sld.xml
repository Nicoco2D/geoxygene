        </Rule>
      </FeatureTypeStyle>
      <FeatureTypeStyle>  
        <Rule>
          <Filter>
            <Or>
              <PropertyIsEqualTo>
                <PropertyName>highway</PropertyName>
                <Literal>motorway</Literal>
              </PropertyIsEqualTo>
              <PropertyIsEqualTo>
                <PropertyName>highway</PropertyName>
                <Literal>motorway_link</Literal>
