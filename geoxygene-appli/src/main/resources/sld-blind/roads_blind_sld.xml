              <CssParameter name="stroke-width">1.75</CssParameter>
              <CssParameter name="stroke-linecap">butt</CssParameter>
              <CssParameter name="stroke-dasharray">4 3</CssParameter>              
            </Stroke>
          </LineSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <And>
              <PropertyIsEqualTo>
