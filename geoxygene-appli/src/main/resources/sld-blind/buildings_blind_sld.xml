<?xml version="1.0" encoding="utf-8"?>
<StyledLayerDescriptor version="1.0.20">
	<Background>
		<Color>#808080</Color>
	</Background>
  <NamedLayer> <!-- BUILDINGS -->
    <Name>buildings</Name>
    <UserStyle>
      <Name>buildings</Name>
      <FeatureTypeStyle>
        <Rule>
          <Filter>
          <And>
            <PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>yes</Literal>
            </PropertyIsEqualTo>
            <PropertyIsNotEqualTo>
              <PropertyName>aeroway</PropertyName>
              <Literal>terminal</Literal>
            </PropertyIsNotEqualTo>
            </And>
          </Filter>
          <PolygonSymbolizer>
			<Stroke>
              <CssParameter name="stroke">#1a1a1a</CssParameter>
              <CssParameter name="stroke-width">0.5</CssParameter>
            </Stroke>
            <Fill> 
			  <CssParameter name="fill-opacity">1</CssParameter>
              <CssParameter name="fill">#262626</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule>
		<Rule> <!-- PUBLIC BUILDINGS -->
          <Filter>
		  <Or>
            <PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>civic</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>public</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>office</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>school</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>hospital</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>university</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
              <PropertyName>building</PropertyName>
              <Literal>cathedral</Literal>
            </PropertyIsEqualTo>
			<PropertyIsEqualTo>
				<PropertyName>building</PropertyName>
				<Literal>chapel</Literal>
            </PropertyIsEqualTo>
		  </Or>			
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill-opacity">1</CssParameter>
              <CssParameter name="fill">#262626</CssParameter>
			  <GraphicFill>
					 <Graphic>
					   <Mark>
						 <WellKnownName>\</WellKnownName>
						 <Fill>
						   <CssParameter name="fill">#1a1a1a</CssParameter>
						   <CssParameter name="fill-opacity">1</CssParameter>
						 </Fill>
					   </Mark>
					   <Size>15</Size>
					 </Graphic>
				</GraphicFill>
            </Fill>
			<Stroke>
              <CssParameter name="stroke">#1a1a1a</CssParameter>
              <CssParameter name="stroke-width">0.5</CssParameter>
            </Stroke>
          </PolygonSymbolizer>
        </Rule>
        <Rule>
          <Filter>
            <PropertyIsEqualTo>
              <PropertyName>aeroway</PropertyName>
              <Literal>hangar</Literal>
            </PropertyIsEqualTo>
          </Filter>
          <PolygonSymbolizer>
            <Fill>
              <CssParameter name="fill">#262626</CssParameter>
            </Fill>
          </PolygonSymbolizer>
        </Rule>
      </FeatureTypeStyle>
    </UserStyle>
  </NamedLayer> 	
</StyledLayerDescriptor>